{
  "name": "funds",
  "version": "0.0.1",
  "description": "Funds OB",
  "private": true,
  "author": "Vincent Boutour",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ViBiOh/funds/issues"
  },
  "homepage": "https://funds.vibioh.fr",
  "dependencies": {
    "chart.js": "3.7.0",
    "classnames": "2.3.1",
    "date-fns": "2.28.0",
    "funtch": "2.5.4",
    "history": "5.2.0",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "4.3.1",
    "react-redux": "7.2.6",
    "redux": "4.1.2",
    "redux-saga": "1.1.3",
    "regenerator-runtime": "0.13.9",
    "url-join": "4.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "CI=true react-scripts test --coverage",
    "eject": "react-scripts eject",
    "style": "eslint --fix \"src/**/*.js{,x}\"",
    "format": "prettier --write \"src/**/*.js{,x}\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb"
    ],
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "src"
          ]
        }
      }
    },
    "env": {
      "jest": true
    },
    "rules": {
      "default-param-last": 0,
      "import/no-extraneous-dependencies": 0,
      "react/function-component-definition": 0,
      "react/jsx-props-no-spreading": 0
    }
  },
  "devDependencies": {
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "8.8.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "prettier": "2.5.1",
    "react-scripts": "5.0.0",
    "react-test-renderer": "17.0.2",
    "sinon": "13.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
